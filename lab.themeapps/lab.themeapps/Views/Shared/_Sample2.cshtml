<script type="text/javascript">
    	$(document).ready(function(){

    	    function hideAll() {
    	        $('#divContainerWrapperWithImg').hide();
    	        $('#divContainerWrapperWithoutImg').hide();
        	    $('#divProfessionalInfo').hide();
        	    $('#divPatientReport').hide();
        	    $('#divPatientReportSurvey').hide();
        	    $('#divPatientReportSurveyWiseChart').hide();
        	    $('#divPatientReportSurveyWiseTable').hide();

        	    $('#divBtnPatientReportSurveyBack').hide();
        	    $('#divBtnPatientReportSurveyWiseTableBack').hide();
        	};

        	$('body').on('click', '.lnkSubMenu', function (e) {
        	    hideAll();
        	    $('#divContainerWrapperWithoutImg').show();
        	    $('#divPatientReport').show();
        	});

        	$('body').on('click', '#lnkPatientReportedOutcomes', function (e) {
        	    hideAll();
        	    $('#divContainerWrapperWithoutImg').show();
        	    $('#divPatientReportSurvey').show();
        	});

        	$('body').on('click', '#lnkHospitalReportedOutcomes', function (e) {
        	    hideAll();
        	    $('#divContainerWrapperWithoutImg').show();
        	    $('#divPatientReportSurvey').show();
        	});

        	$('body').on('click', '#lnkIndividualPatientOutcomes', function (e) {
        	    hideAll();
        	    $('#divContainerWrapperWithoutImg').show();
        	    $('#divPatientReportSurveyWiseTable').show();
        	});

        	$('body').on('click', '.btnPatientReportSurvey', function (e) {
        	    hideAll();
        	    $('#divContainerWrapperWithoutImg').show();
        	    $('#divPatientReportSurveyWiseChart').show();
        	    $('#divBtnPatientReportSurveyBack').show();
        	});

        	$('body').on('click', '.btnPatientReportSurveyBack', function (e) {
        	    hideAll();
        	    $('#divContainerWrapperWithoutImg').show();
        	    $('#divPatientReportSurvey').show();
        	});

        	$('body').on('click', '.btnPatientReportBack', function (e) {
        	    hideAll();
        	    $('#divContainerWrapperWithoutImg').show();
        	    $('#divPatientReport').show();
        	});

        	$('body').on('click', '.btnPatientReportSurveyWiseTableBack', function (e) {
        	    hideAll();
        	    $('#divContainerWrapperWithoutImg').show();
        	    $('#divPatientReportSurveyWiseTable').show();
        	});

        	$('body').on('click', '.btnPatientReport', function (e) {
        	    hideAll();
        	    $('#divContainerWrapperWithoutImg').show();
        	    $('#divPatientReportSurveyWiseChart').show();
        	    $('#divBtnPatientReportSurveyWiseTableBack').show();
        	});

    	});
</script>